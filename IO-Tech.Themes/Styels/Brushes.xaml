<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--Layout Colors-->
    <Color x:Key="BaseColor">#171819</Color>
    <Color x:Key="MainColor">#2d3134</Color>
    <Color x:Key="BorderColor">#41454a</Color>

    <!--Text Colors-->
    <Color x:Key="PrimaryTextColor">#dadedd</Color>
    <Color x:Key="SecondaryTextColor">#dadedd</Color>

    <!--Selection Colors-->
    <Color x:Key="SelectionBgColor">#344151</Color>
    <Color x:Key="SelectionBorderColor">#6087b2</Color>

<!--Highlight Colors-->
    <Color x:Key="AccentColor">#00aeef</Color>
    <Color x:Key="YellowColor">#f5cc60</Color>
    <Color x:Key="RedColor">#e93941</Color>
    <Color x:Key="GreenColor">#0b9e57</Color>
    <Color x:Key="NeutralColor">#ccd9e3</Color>




</ResourceDictionary>